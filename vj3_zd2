import math
def funk(n):
    n=int(input('broj-'))
    x=n*1/3
    a=5-x
    print('x je {},a je {}.'.format(x,a))
    return
funk(5)

n1=200
x1=n1*1/3
a1=5-x1
print('x1 je {},a a1 je {}'.format(x1,a1))

n2=2000
x2=n2*1/3
a2=5-x2
print('x2 je {},a a2 je {}'.format(x2,a2))

